{
  "expo": {
    "name": "NicaWood",
    "slug": "NicaWood",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/LogoWithoutBG.png",
